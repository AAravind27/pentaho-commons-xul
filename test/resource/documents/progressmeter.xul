<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<window id="general-datasource-window"
	title="Testing progressmeters for SWT, Swing" orient="vertical" height="80" width="400"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script src="org.pentaho.ui.xul.test.swing.ProgressHandler" id="progressHandler"/>
  <hbox id="all-box">
    <progressmeter id="my-progressmeter" mode="determined" value="10" flex="1"/>
    <button label="Hit me to advance" onclick="progressHandler.addProgress();" />
    <progressmeter mode="undetermined" flex="1"/>
  </hbox>
</window>